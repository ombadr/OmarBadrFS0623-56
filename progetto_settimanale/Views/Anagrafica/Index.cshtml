@model IEnumerable<progetto_settimanale.Models.Anagrafica>

@{
    ViewData["Title"] = "Anagrafica";
}

<h2>Anagrafica</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Cognome</th>
            <th>Nome</th>
            <th>Indirizzo</th>
            <th>Città</th>
            <th>CAP</th>
            <th>Codice Fiscale</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.idanagrafica</td>
                <td>@item.Cognome</td>
                <td>@item.Nome</td>
                <td>@item.Indirizzo</td>
                <td>@item.Città</td>
                <td>@item.CAP</td>
                <td>@item.Cod_Fisc</td>
            </tr>
        }
    </tbody>
</table>